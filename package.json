{
  "name": "cors-proxy-webpack-plugin",
  "version": "0.0.5",
  "description": "a simple cors proxy solution for cross-domain apis in the development environment",
  "main": "dist/main.js",
  "files": [
    "src",
    "bin",
    "dist"
  ],
  "bin": {
    "cors-proxy-server": "bin/launcher.js"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=production webpack --config webpack.config.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ukari/cors-proxy-webpack-plugin.git"
  },
  "keywords": [
    "webpack-plugin",
    "cors-proxy"
  ],
  "author": "muromi ukari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ukari/cors-proxy-webpack-plugin/issues"
  },
  "homepage": "https://github.com/ukari/cors-proxy-webpack-plugin#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^5.0.0-alpha.8",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "node-fetch": "^2.3.0",
    "webpack-log": "^2.0.0"
  }
}
